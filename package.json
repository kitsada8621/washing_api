{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "cross-env NODE_ENV=development nodemon ./bin/www",
    "db:create": "npx sequelize-cli db:create --env=production",
    "db:drop": "npx sequelize-cli db:drop --env=production",
    "migrate": "npx sequelize-cli db:migrate --env=production",
    "seed": "npx sequelize-cli db:seed:all --env=production"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "express-fileupload": "^1.4.0",
    "form-data": "^4.0.0",
    "jimp": "^0.22.8",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "moment": "^2.29.4",
    "morgan": "~1.9.1",
    "mysql2": "^3.5.0",
    "node-cron": "^3.0.2",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.1"
  }
}